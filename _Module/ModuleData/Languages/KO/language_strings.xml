    <string id="Dramalord015" text="{HERO1.LINK} 및 {HERO2.LINK}은(는) 이제 {RELATIONSHIP}" />
    <string id="Dramalord015" text="{HERO1.LINK} 및 {HERO2.LINK} 또는 더 이상 {RELATIONSHIP}이 아님" />
