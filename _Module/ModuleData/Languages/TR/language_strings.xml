  <strings>
    <!-- General Text -->
    <string id="Dramalord001" text="Size şahsi bir şey sorabilir miyim?" />
    <string id="Dramalord002" text="Elbette. Ne bilmek istiyorsunuz?" />
    <string id="Dramalord003" text="Sizinle bu tarz şeyleri konuşmak için kendimi yeterince rahat hissetmiyorum." />
    <string id="Dramalord004" text="Başka bir şey hakkında konuşalım..." />
    <string id="Dramalord005" text="Nasıl isterseniz." />
    <!-- Attraction Text -->
    <string id="Dramalord006" text="Ne tarz insanlardan hoşlanıyorsunuz?" />
    <string id="Dramalord007" text="Gördüğünüz şey hoşunuza gitti mi?" />
    <string id="Dramalord008" text="Ben kadınları {RATING_WOMEN} ve erkekleri {RATING_MEN} buluyorum. Partnerimin {RATING_WEIGHT} ve {RATING_BUILD} olmasını tercih ederim. Benim için en uygunu {RATING_CULTURE} {RATING_AGE} yaşlarında olması." />
    <string id="Dramalord009" text="Hmm... Bence sen {RATING_TOTAL}!" />
    <!-- Attraction general words -->
    <string id="Dramalord010" text="çekici" />
    <string id="Dramalord011" text="iyi" />
    <string id="Dramalord012" text="iğrenç" />
