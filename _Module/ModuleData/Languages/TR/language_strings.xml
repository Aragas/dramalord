    <!-- Attraction player rating -->
    <string id="Dramalord020" text="çok iğrençsin!" />
    <string id="Dramalord021" text="iğrenç görünüyorsun!" />
    <string id="Dramalord022" text="baya çirkinsin!" />
    <string id="Dramalord023" text="çirkinsin." />
    <string id="Dramalord024" text="çok da güzel değilsin.." />
    <string id="Dramalord025" text="idare edersin.." />
    <string id="Dramalord026" text="iyisin.." />
    <string id="Dramalord027" text="hoşsun.." />
    <string id="Dramalord028" text="muhteşemsin!" />
    <string id="Dramalord029" text="çok çekicisin!" />
    <!-- Emotion text -->
    <string id="Dramalord030" text="Benim hakkımda ne düşünüyorsun?" />
    <string id="Dramalord031" text="Ben {EMOTION_SCORE}" />
    <!-- Emotion words -->
    <string id="Dramalord032" text="sana karşı kin besliyorum!" />
    <string id="Dramalord033" text="senden nefret ediyorum!" />
    <string id="Dramalord034" text="seni görmek istemiyorum." />
    <string id="Dramalord035" text="seni sevmiyorum." />
    <string id="Dramalord036" text="sana çok da hayran değilim." />
    <string id="Dramalord037" text="sana saygı duyuyorum." />
    <string id="Dramalord038" text="senden hoşlanıyorum.." />
    <string id="Dramalord039" text="sanırım sana aşık oluyorum.." />
    <string id="Dramalord040" text="seni düşünmeden edemiyorum.." />
    <string id="Dramalord041" text="sana sırılsıklam aşığım." />
    <!-- Action text -->
    <string id="Dramalord042" text="Benimle, şey..." />
    <string id="Dramalord043" text="Evet, seninle?" />
    <string id="Dramalord044" text="...birlikte daha sakin bir yere gidelim mi?" />
    <string id="Dramalord045" text="Hmm... pekala, hadi öyle yapalım." />
    <string id="Dramalord046" text="Hayır. Böyle bir arzum yok.." />
    <string id="Dramalord047" text="...biraz benimle gelir misin?" />
    <string id="Dramalord050" text="...'biz' için boşanmak ister misin?" />
    <string id="Dramalord051" text="...benimle evlenir misin?" />
    <string id="Dramalord053" text="Seninle niçin geleyim ki?" />
    <string id="Dramalord054" text="Sanırım atımı ahıra bırakmam gerekiyor.." />
    <string id="Dramalord055" text="Kendimi yorgun hissediyorum. Sonra bakarız.." />
    <string id="Dramalord056" text="Rüzgar gibi hızlı olacağız!" />
    <string id="Dramalord057" text="Bütün gün kalede oturmak can sıkıcı.." />
    <string id="Dramalord058" text="Seni dikkatimle boğacağım!" />
    <string id="Dramalord059" text="Varlığından onur duyuyorum.." />
    <string id="Dramalord060" text="Zavallı atlar bütün gün ayakta durmaktan başka bir şey yapmıyorlar.." />
    <string id="Dramalord061" text="Evliliğimden niçin vazgeçeyim ki?" />
    <string id="Dramalord068" text="Senin için evliliğimden vazgeçecek değilim!" />
    <string id="Dramalord069" text="Seni bu evliliğe yakalanmış görmeye dayanamıyorum." />
    <string id="Dramalord070" text="...benimle yürüyüşe çıkar mısın?" />
    <string id="Dramalord071" text="Hadi gidelim!" />
    <string id="Dramalord072" text="Boşver." />
    <string id="Dramalord073" text="Harikaydı!" />
    <string id="Dramalord074" text="Sıkıcıydı." />
    <string id="Dramalord078" text="Uzun vadede bu daha iyi bir seçim." />
    <string id="Dramalord079" text="Bu evliliğe yaptığın tüm yatırım boşuna!" />
    <string id="Dramalord080" text="Bu evliliğin şerefine leke sürdün." />
    <string id="Dramalord081" text="Senin gibi biriyle evlenemem." />
    <string id="Dramalord082" text="Seninle neden evleneyim ki?" />
    <string id="Dramalord083" text="Birlikte yepyeni maceralara atılırız!" />
    <string id="Dramalord084" text="Birlikte, geleceğimizi nasıl inşa edebileceğimizi hayal etsene." />
    <string id="Dramalord085" text="Biz, birbirimizle her şeyimizi paylaşmalıyız." />
    <string id="Dramalord086" text="Bizim için böyle yapmak büyük bir onur olur." />
    <string id="Dramalord087" text="Ben sen olmadan yaşamayamam ki!" />
    <string id="Dramalord088" text="Seninle niçin zaman geçireyim ki?" />
    <string id="Dramalord089" text="Beni sıkıyorsun." />
    <string id="Dramalord090" text="Sana anlatacak heyecan verici hikayelerim var!" />
    <string id="Dramalord091" text="Biraz eğlence istemez misin?" />
    <string id="Dramalord092" text="Sana bir şeyler ısmarlayabilirim." />
    <string id="Dramalord093" text="Seninle zaman geçirmek benim için büyük bir onur." />
    <string id="Dramalord094" text="Sana yalvarıyorum! Çok sıkıldım!" />
    <string id="Dramalord095" text="Ben evli değilim..." />
    <string id="Dramalord098" text="Ben, şey..." />
    <string id="Dramalord102" text="Evet, sen ne?" />
    <string id="Dramalord103" text="...sana bir şey vermek istiyorum." />
    <string id="Dramalord104" text="...bu aşk hikayesini bitirmek istiyorum." />
    <string id="Dramalord105" text="...bu evliliğe son vermek istiyorum." />
    <string id="Dramalord106" text="Ah! Bunu iyi bir şekilde saklayacağım." />
    <string id="Dramalord107" text="Oh. Sonunda bu iş bitti!" />
    <string id="Dramalord108" text="Ah. Buna hiç hazır değildim. Ben.. Biraz yalnız kalmak istiyorum..." />
    <string id="Dramalord109" text="Ne? Seni alçak! Seni bir daha görmek istemiyorum, asla!" />
    <string id="Dramalord110" text="Aman tanrım... En karanlık kabusum gerçek oldu... Ben sensiz nasıl yaşayacağım..." />
    <string id="Dramalord111" text="Sen ve ben...Şimdi biraz eğlence zamanı..." />
    <string id="Dramalord112" text="Ne, hayır! Lütfen benden uzak dur!" />
